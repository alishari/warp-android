<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.schibsted.nmp.warp.components.WarpButtonView
        android:id="@+id/warp_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:warpButtonStyle="primary"
        app:text="Meowza"
        app:loading="false"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <com.schibsted.nmp.warp.components.WarpButtonView
        android:id="@+id/warp_button_two"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:warpButtonStyle="secondary"
        app:text="Meowings"
        app:loading="false"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/warp_button"/>

</androidx.constraintlayout.widget.ConstraintLayout>